		<AndroidMavenLibrary Include="com.squareup.wire:wire-moshi-adapter" Version="4.7.2" Bind="False" />
		<AndroidMavenLibrary Include="com.squareup.wire:wire-runtime" Version="4.7.2" Bind="False" />
		<AndroidMavenLibrary Include="com.squareup.wire:wire-runtime-jvm" Version="4.7.2" Bind="False" />

		<AndroidMavenLibrary Include="io.ktor:ktor-client-core" Version="2.3.2" Bind="False" />
